#include<stdio.h>
int main(){
int n , m , i , j , k ,t , x , sum = 0;
while(1){
    scanf("%d%d" , &n , &m);
    if(n>m){
        t=m;
        m=n;
        n=t;
    }
    if(n==0 || n<0 || m<0 || m==0){
        break;
    }
    sum = 0;
    for(j=n ; j<=m ; j++){
        printf("%d " , j);
        sum+=j;
    }
    printf("sum =%d" , sum);
    printf("\n");
}
return 0;
}
