#include<stdio.h>
#include<math.h>
int main(){
int i , n , x , y , z, cnt = 0 ;
scanf("%d" , &n);
for(i=1 ; i<=n ; i++){
        cnt = 0;
    scanf("%d" , &x);
    while(x>0){
        y = x%2;
        x = x/2;
        if(y==1) cnt++;

    }
    z = pow(2 , cnt)-1;
    printf("%d\n" , z);
}

return 0;
}
