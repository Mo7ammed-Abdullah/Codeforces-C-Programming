#include<stdio.h>
int main(){
int i , x , n , y ,s=0 ,s1 = 0, a , b ;
scanf("%d%d%d" , &n , &a , &b);
for(i=1 ; i<=n ; i++){
        x=i;
        s = 0;
    while(x>0){
        y = x%10;
        s+=y;
        x=x/10;
    }
    if(s>=a && s<=b){
        s1+=i;
    }
}
printf("%d" , s1);

return 0;
}
