#include<stdio.h>
int main(){

int n , i , x , a , ev=0 , od=0 , po=0 , ne=0;
scanf("%d" , &n);

for(i=1;i<=n;i++)
{
    scanf("%d" , &a );

  if(a%2==0  ){
    ev++;
  }

  if(a%2!=0 ){
    od++;
  }
  if(a>0){
    po++;
  }
  if(a<0 ){
    ne++;
  }
}
printf("Even: %d\n" , ev);
printf("Odd: %d\n" , od);
printf("Positive: %d\n" , po);
printf("Negative: %d\n" , ne);




return 0;
}
