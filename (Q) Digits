#include<stdio.h>
int main(){
int n , x , i , j , y , y1 , z;
scanf("%d"  , &n);
for(i=1 ; i<=n ; i++){
    scanf("%d" , &x);
    z = x;
    y1 = z%10;
    printf("%d" , y1);
    z = z/10;
    while(z>0){
        y = z%10;
        printf(" %d" , y);
        z = z/10;
       }
printf("\n");
}
return 0;
}
